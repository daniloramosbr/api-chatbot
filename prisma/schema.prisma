generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id   String @id @default(uuid())
  name String @unique
}

model Pizza {
  id        Int      @id @default(autoincrement())
  user      String
  sabor     String
  quant     Int
  createdAt DateTime @default(now())
}
